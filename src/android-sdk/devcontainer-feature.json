{
    "name": "Android SDK",
    "id": "android-sdk",
    "version": "0.0.2",
    "description": "Android SDK",
    "containerEnv": {
        "ANDROID_SDK_ROOT": "/usr/local/lib/android",
        "ANDROID_HOME": "$ANDROID_SDK_ROOT",
        "PATH": "$PATH:$ANDROID_SDK_ROOT/cmdline-tools/latest/bin:$ANDROID_SDK_ROOT/platform-tools"
